{
  "name": "kratos",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Christian Froehlingsdorf <chris@5cf.de>",
  "license": "SEE LICENSE IN ./LICENSE",
  "private": false,
  "scripts": {
    "tsv": "tsc -v",
    "prepublishOnly": "yarn build",
    "build": "rm -rf dist/ && tsc -p tsconfig.json",
    "_start": "DEBUG=service:* node dist/example/index.js",
    "start": "yarn build && yarn _start",
    "start:bin": "yarn build && node bin/kratos.js -l ./baseConfig.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "yarn:openssl": "LDFLAGS='-L/usr/local/opt/openssl/lib' CPPFLAGS='-I/usr/local/opt/openssl/include' yarn",
    "test:only": "TS_NODE_PROJECT='tsconfig.test.json' _mocha",
    "test": "yarn build && yarn test:only",
    "test:log": "yarn build && DEBUG=service:* TS_NODE_PROJECT='tsconfig.test.json' _mocha"
  },
  "keywords": [],
  "dependencies": {
    "bluebird": "~3.5.4",
    "body-parser": "~1.19.0",
    "commander": "~2.20.0",
    "cors": "~2.8.5",
    "debug": "~4.1.1",
    "express": "~4.16.4",
    "prom-client": "^11.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "~3.5.26",
    "@types/cors": "~2.8.5",
    "@types/debug": "~4.1.4",
    "@types/express": "~4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "~12.0.0",
    "mocha": "^6.1.4",
    "request": "^2.88.0",
    "tslint": "~5.16.0",
    "typescript": "~3.4.5"
  },
  "preferGlobal": true,
  "bin": {
    "kratos": "bin/kratos.js"
  }
}
